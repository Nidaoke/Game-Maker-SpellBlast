 { button_delay=12 cursorY++ if cursorY&gt;maxY {cursorY=0} }
 
if (confirmkey) and button_delay=0
 {
    if cursorY=0 and global.tutorial_lock=0  {lives=5 room_goto(rm_gameplay) }
    if cursorY=1 {lives=5 room_goto(rm_tutorial)}
    if cursorY=2 {room_goto(rm_options)}
    if cursorY=3 {game_end()}
 }
