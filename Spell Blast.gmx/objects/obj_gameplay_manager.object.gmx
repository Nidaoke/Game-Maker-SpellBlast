life_reward=0
boss_present=0
shake=0
boss_shake=0

BGM_index=0
BossBGM_index=0
large_enemy_delay=0

//spawn a letter after so many enemies have been spawned.
force_letter_spawn=0
start_boss_correct_match=0

letter_spawn_delay=round(room_speed*(3+irandom(15) ))
global.force_letter_spawnMAX=5
large_enemy_delay=0

TLW=0 match_wrong=0
